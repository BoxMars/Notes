function answer=simpson(n)
    answer=f(0);
    d=10.0/n;
    for i=1:1:2^(n-1)
        answer=answer+f(10*(2*i-1)/2^n)*4;
    end
    for i=1:1:2^(n-1)-1
        answer=answer+f(10*(2*i)/2^n)*2;
    end
    answer=answer+f(10);
    answer=1.0/(3*2^n)*answer;
end

